{
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "node": true
    },
    "extends": ["airbnb"],
    "rules": {
        "no-console": 0,
        "eol-last": 0,
        "no-tabs": 0,
        "indent": 0,
				// Should be disabled in production
				"no-unused-vars": 0,
				"import/prefer-default-export": 0,
				"react/jsx-filename-extension": 0,
				"global-require": 0,
				"react/jsx-indent-props": 0,
				"react/jsx-indent": 0,
				"import/no-unresolved": 0,
				"import/extensions": 0,
				"react/no-did-mount-set-state": 0
    }
}