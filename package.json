  "betterScripts": {
    "build:prod": {
      "command": "concurrently \" npm run build:js \" \" node server/index.js \"",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "server:*,webpack:*",
        "DEBUG_COLORS": true,
        "BASE_URI": "http://35.187.105.249:3000/v1"
      }
    },
    "build:dev": {
      "command": "nodemon --config config/nodemon.webpack.json"
    }
  },
